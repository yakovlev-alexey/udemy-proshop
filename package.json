{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN ProShop eCommerce application",
  "main": "backend/server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
